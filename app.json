{
  "expo": {
    "name": "HydraCare",
    "slug": "HydraCare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "plugins": ["expo-router", "expo-updates"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a3956965-992c-4084-8d70-27f5c02de8ac"
      }
    },
    "android": {
      "package": "com.himitris.HydraCare"
    }
  }
}
